# Glimpse Mobile App Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:8080/api/v1
EXPO_PUBLIC_WEBSOCKET_URL=ws://localhost:8080

# Authentication (Clerk)
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key

# Payment
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Kakao
EXPO_PUBLIC_KAKAO_APP_KEY=your_kakao_app_key

# Google Maps (for Android)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Sentry (optional)
EXPO_PUBLIC_SENTRY_DSN=https://your_sentry_dsn@sentry.io/project_id