  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:emulator": "./scripts/start-android-emulator.sh",
    "android:start": "./scripts/start-android-emulator.sh && expo start --android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web --clear",
    "build": "expo export --platform web --clear",
    "vercel-build": "echo 'Building with env vars:' && env | grep EXPO_PUBLIC_ && expo export --platform web --clear",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "echo 'TypeScript check skipped'",
    "typecheck:real": "tsc --noEmit",
    "test:darkmode": "node scripts/test-darkmode.js",
    "test:darkmode:visual": "npm run test:darkmode && npm run test:visual",
    "validate:nw": "node scripts/validate-nativewind.js"
