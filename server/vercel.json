{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "api/minimal.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/health.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/debug.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/minimal",
      "dest": "/api/minimal"
    },
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/debug",
      "dest": "/api/debug"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}