# Supabase Database Configuration
# 이 파일은 Supabase 연결 설정 예시입니다.
# 실제 값은 Vercel 환경 변수에 설정해야 합니다.

# Supabase PostgreSQL Connection
DATABASE_URL="postgresql://[username]:[password]@[host]:[port]/[database]?pgbouncer=true&connection_limit=1"

# 예시 형식:
# DATABASE_URL="postgresql://postgres.abcdefghijklmnop:[password]@aws-0-ap-northeast-1.pooler.supabase.com:5432/postgres?pgbouncer=true&connection_limit=1"

# 서버 환경 변수
NODE_ENV=production
PORT=3001

# JWT Secret (Vercel에서 설정)
JWT_SECRET=your_production_jwt_secret

# Encryption Key (Vercel에서 설정)
ENCRYPTION_KEY=your_production_encryption_key

# Clerk Authentication (Vercel에서 설정)
CLERK_SECRET_KEY=your_production_clerk_secret
CLERK_PUBLISHABLE_KEY=your_production_clerk_publishable

# AWS S3 (optional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=ap-northeast-2
AWS_S3_BUCKET_NAME=

# Firebase Admin (optional)
FIREBASE_PROJECT_ID=
FIREBASE_CLIENT_EMAIL=
FIREBASE_PRIVATE_KEY=

# Redis (optional for production)
REDIS_URL=

# Dev Auth (disabled in production)
DEV_AUTH_ENABLED=false