{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "node scripts/load-env.js 'prisma generate && nest build && node scripts/copy-locales.js'",
    "postbuild": "node scripts/copy-locales.js",
    "build:railway": "prisma generate && nest build",
    "vercel-build": "echo 'Vercel build starting...' && prisma generate && nest build",
    "deploy:db": "node scripts/deploy-db.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node scripts/load-env.js 'nest start'",
    "dev": "node scripts/load-env.js 'nest start --watch'",
    "start:dev": "node scripts/load-env.js 'nest start --watch'",
    "start:debug": "node scripts/load-env.js 'nest start --debug --watch'",
    "start:prod": "node scripts/load-env.js 'node dist/main'",
    "start:railway": "node dist/main",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "npx tsx prisma/seed.ts",
    "db:seed": "npx tsx prisma/seed.ts",
    "seed:mock": "npx tsx prisma/migrate-mockdata.ts",
    "seed:domains": "npx tsx src/scripts/seed-company-domains.ts",
    "seed:english": "npx tsx src/scripts/seed-english-data.ts",
    "seed:all": "npm run seed:domains && npm run seed:english",
    "seed:test": "npx tsx prisma/seed-test.ts",
    "seed:extended": "npx tsx prisma/seed-extended.ts",
    "seed:local": "npx tsx prisma/seed-local-unified.ts",
    "seed:railway": "npx tsx prisma/seed-railway.ts",
    "seed:railway:quick": "npx tsx prisma/seed-railway-quick.ts",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:pull": "npx prisma db pull",
    "db:studio": "npx prisma studio",
    "db:migrate:railway": "npx prisma generate && npx prisma db push",
    "db:reset": "npx prisma db push --force-reset",
    "db:setup:local": "npm run db:generate && npm run db:push && npm run seed:local",
    "db:setup:railway": "npm run db:migrate:railway && npm run seed:railway",
    "db:setup:railway:quick": "npm run db:migrate:railway && npm run seed:railway:quick"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.864.0",
    "@clerk/backend": "^2.13.0",
    "@clerk/clerk-sdk-node": "^5.1.6",
    "@nestjs-modules/mailer": "^1.11.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.5",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.5",
    "@prisma/client": "^5.22.0",
    "@sendgrid/mail": "^8.1.5",
    "@sentry/node": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.1.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^11.0.7",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.1.0",
    "i18next": "^25.3.4",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.8.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.22.0",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "stripe": "^18.4.0",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.8.0",
    "uuid": "^7.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@playwright/test": "^1.54.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.17.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@vercel/node": "^3.0.31",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "playwright": "^1.54.2",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "prisma": {
    "seed": false
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
