{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": [
      "ES2021"
    ]
  },
  "include": [
    "src/**/*",
    "test/**/*",
    "!src/scripts/migrate-to-structured.ts",
    "!src/scripts/register-all-my-info.ts",
    "!src/scripts/test-all-registrations.ts",
    "!src/scripts/upgrade-to-premium.ts"
  ],
  "exclude": [
    "eslint.config.mjs",
    "node_modules",
    "dist",
    "src/scripts/migrate-to-structured.ts",
    "src/scripts/register-all-my-info.ts",
    "src/scripts/test-all-registrations.ts",
    "src/scripts/upgrade-to-premium.ts"
  ]
}
